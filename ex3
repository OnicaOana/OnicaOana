#include<iostream>
using namespace std;
int main()
{
    int x,y,a,b;
    cout<<"x=";cin>>x;
    cout<<"y=";cin>>y;
    a=x;
    b=y;
    while(x!=y)
        if(x>y)
        x=x-y;
    else
        y=y-x;
    if(x==1)
        cout<<"Numerele sunt prime intre ele";
    else
        cout<<"Numerele nu sunt prime intre ele";
}
